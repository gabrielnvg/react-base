    "@storybook/react": "^5.3.9",
    "acorn": "7.1.1",
    "ajv": "6.12.3",
    "autoprefixer": "9.7.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "browser-sync": "2.26.7",
    "commander": "^4.1.0",
    "concurrently": "5.0.2",
    "connect-history-api-fallback": "1.6.0",
    "cross-env": "^6.0.3",
    "css-loader": "3.4.2",
