    "node-sass": "^4.13.1",
    "npm-check-updates": "^4.0.1",
    "open-cli": "5.0.0",
    "postcss-loader": "3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-scss": "^2.0.0",
